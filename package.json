{
	"name": "@lucaconlaq/grape-cola",
	"version": "0.1.0",
	"description": "A thin wrapper that gives grpc-js a better DX",
	"keywords": ["grpc", "grpc-js", "middleware", "typescript"],
	"license": "MIT",
	"author": "lucaconlaq",
	"repository": {
		"type": "git",
		"url": "https://github.com/lucaconlaq/grape-cola.git"
	},
	"homepage": "https://github.com/lucaconlaq/grape-cola#readme",
	"bugs": {
		"url": "https://github.com/lucaconlaq/grape-cola/issues"
	},
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": ["dist"],
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"build": "tsc",
		"test": "vitest run",
		"check": "tsc --noEmit && biome check src test",
		"format": "biome format --write src test",
		"lint": "biome lint --write src test",
		"release:patch": "npm run build && npm version patch && npm publish --access public",
		"release:minor": "npm run build && npm version minor && npm publish --access public",
		"release:major": "npm run build && npm version major && npm publish --access public",
		"example:gen": "tsx example/script/gen-grpc.ts",
		"example:server:build": "tsc -p example/server/tsconfig.json",
		"example:server:run": "tsx example/server/main.ts",
		"example:server:dev": "tsx watch example/server/main.ts",
		"example:client:build": "tsc -p example/client/tsconfig.json",
		"example:client:run": "tsx example/client/cli.tsx",
		"example:test": "vitest run -c example/vitest.config.ts"
	},
	"dependencies": {
		"@grpc/grpc-js": "^1.12.6"
	},
	"peerDependencies": {
		"pino": "^10.0.0",
		"pino-pretty": "^13.0.0"
	},
	"peerDependenciesMeta": {
		"pino": {
			"optional": true
		},
		"pino-pretty": {
			"optional": true
		}
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/pino": "^7.0.4",
		"@types/react": "^18.3.28",
		"google-protobuf": "^3.21.4",
		"grpc-tools": "^1.13.1",
		"grpc_tools_node_protoc_ts": "^5.3.3",
		"ink": "^5.0.1",
		"ink-gradient": "^3.0.0",
		"ink-text-input": "^6.0.0",
		"jsonwebtoken": "^9.0.3",
		"pino": "^10.3.1",
		"pino-pretty": "^13.1.3",
		"react": "^18.3.1",
		"tsx": "^4.19.3",
		"typescript": "^5.7.3",
		"vitest": "^4.0.18"
	}
}
